<!DOCTYPE html>
<html>
<head>
    <title>Home</title>
    <meta charset="utf-8" />
    <meta name="description" content="" />
    <meta name="keywords" content="" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link href="https://fonts.googleapis.com/css?family=Playfair+Display:400,400i,700,700i,900,900i" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Roboto:100,100i,300,300i,400,400i,500,500i,700,700i,900,900i" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="css/main.css" />
</head>
<body class="animsition">
<div class="login" id="page">
    <% include partials/header.ejs %>
    <section class="boxed-sm">
        <div class="container">
            <form action="/auth/signup" class="col-md-12" method="post">
                <div class="row">
                <div class="col-md-6 form-group">
                    <label for="">Username :</label>
                    <input autofocus class="form-control" type="text" name="username">
                </div>
                </div>
                <div class="row">
                <div class="col-md-6 form-group">
                    <label for="">Password : </label>
                    <input class="form-control" type="password" name="password">
                </div>
                </div>

                <h4>Detalji za isporuku</h4>
                <div class="row">
                    <div class="col-md-6 form-group">
                        <div class="form-group Orchid-form no-radius">
                            <label>Ime *</label>
                            <input class="form-control" type="text" name="firstname">
                        </div>
                    </div>
                    <div class="col-md-6 form-group">
                        <div class="form-group Orchid-form no-radius">
                            <label>Prezime *</label>
                            <input class="form-control" type="text" name="lastname">
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-6">
                        <div class="form-group Orchid-form no-radius">
                            <label>E-mail adresa *</label>
                            <input class="form-control" type="email" name="email">
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group Orchid-form no-radius">
                            <label>Broj telefona *</label>
                            <input class="form-control" type="text" name="phone">
                        </div>
                    </div>
                </div>
                <div class="row">

                    <div class="col-md-6">
                        <div class="form-group Orchid-form no-radius">
                            <label>Adresa *</label>
                            <input class="form-control" type="text" name="address">
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group Orchid-form no-radius">
                            <label>Grad/Mesto *</label>
                            <input class="form-control" type="text"  name="city">
                        </div>
                    </div>
                </div>
                <div class="row">


                </div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="woocommerce-account-fields">
                            <div class="checkbox">
                                <label>
                                    <input type="checkbox"> Registrujte se?
                                </label>
                            </div>
                        </div>
                    </div>
                </div>

                <input type="submit" value="Signup" class="btn secondary white">
                <a href="/login" class="btn btn-link">Login</a>
            </form>
        </div>

    </section>
</div>
<% include partials/footer.ejs %>
<script src="js/library/jquery.min.js"></script>
<script src="js/library/bootstrap.min.js"></script>
<script src="js/function-check-viewport.js"></script>
<script src="js/library/slick.min.js"></script>
<script src="js/library/select2.full.min.js"></script>
<script src="js/library/imagesloaded.pkgd.min.js"></script>
<script src="js/library/jquery.mmenu.all.min.js"></script>
<script src="js/library/rellax.min.js"></script>
<script src="js/library/isotope.pkgd.min.js"></script>
<script src="js/library/bootstrap-notify.min.js"></script>
<script src="js/library/bootstrap-slider.js"></script>
<script src="js/library/in-view.min.js"></script>
<script src="js/library/countUp.js"></script>
<script src="js/library/animsition.min.js"></script>
<script src="js/global.js"></script>
<script src="js/config-mm-menu.js"></script>
<script src="js/config-set-bg-blog-thumb.js"></script>
<script src="js/config-accrodion.js">


</script>
</body>
</html>



